{
  "name": "smartpay-sdk-nodejs",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "version": "0.0.1",
  "description": "Smartpay SDK NodeJS",
  "main": "build-cjs/index.js",
  "exports": {
    ".": [
      {
        "default": "./build/index.js"
      }
    ]
  },
  "types": "./build/index.d.ts",
  "scripts": {
    "format": "prettier --write 'src/**/*.ts'",
    "schema": "jtd-codegen src/schemas/checkout-session-payload.jtd.json --typescript-out src/schemas",
    "build:esm": "tsc --build tsconfig.json",
    "build:cjs": "tsc --build tsconfig-cjs.json",
    "build": "yarn build:esm && yarn build:cjs",
    "pretest": "yarn build",
    "test:unit": "tape test/unit.js",
    "test:nock": "tape test/nock.js",
    "test:live": "tape test/live.js",
    "test": "yarn test:unit && yarn test:nock && yarn test:live"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartpay-co/checkout-sdk.git"
  },
  "keywords": [
    "bnpl",
    "smartpay",
    "sdk",
    "node"
  ],
  "author": "Smartpay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartpay-co/checkout-sdk/issues"
  },
  "homepage": "https://github.com/smartpay-co/checkout-sdk#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-webpack-plugin": "^2.5.2",
    "nock": "^13.1.1",
    "prettier": "^2.3.2",
    "tape": "^5.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0",
    "jtd": "^0.1.1",
    "query-string": "^7.0.1"
  }
}
